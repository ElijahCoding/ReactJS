mn":30}},{"generated":{"line":117,"column":16},"source":"components/App.js","original":{"line":48,"column":32}},{"name":"state","generated":{"line":117,"column":21},"source":"components/App.js","original":{"line":48,"column":37}},{"generated":{"line":117,"column":26},"source":"components/App.js","original":{"line":48,"column":32}},{"name":"tracks","generated":{"line":117,"column":27},"source":"components/App.js","original":{"line":48,"column":43}},{"generated":{"line":118,"column":0},"source":"components/App.js","original":{"line":48,"column":16}},{"generated":{"line":118,"column":8},"source":"components/App.js","original":{"line":39,"column":12}},{"generated":{"line":118,"column":9},"source":"components/App.js","original":{"line":38,"column":8}},{"generated":{"line":119,"column":0},"source":"components/App.js","original":{"line":51,"column":5}},{"name":"Component","generated":{"line":123,"column":2},"source":"components/App.js","original":{"line":7,"column":18}},{"name":"App","generated":{"line":125,"column":15},"source":"components/App.js","original":{"line":54,"column":15}}],"sources":{"components/App.js":"import React, { Component } from 'react'\nimport Artist from './Artist'\nimport Tracks from './Tracks'\n\nconst API_ADDRESS = 'https://spotify-api-wrapper.appspot.com'\n\nclass App extends Component {\n    state = { artistQuery: '', artist: null, tracks: [] }\n\n    handleKeyPress = event => {\n        if (event.key === 'Enter') {\n            this.searchArtist()\n        }\n    }\n\n    updateArtistQuery = event => {\n        this.setState({ artistQuery: event.target.value })\n    }\n\n    searchArtist = () => {\n        fetch(`${API_ADDRESS}/artist/${this.state.artistQuery}`)\n        .then(response => response.json())\n        .then(json => {\n            if (json.artists.total > 0) {\n                const artist = json.artists.items[0]\n                this.setState({ artist })\n\n                fetch(`${API_ADDRESS}/artist/${artist.id}/top-tracks`)\n                .then(response => response.json())\n                .then(json => this.setState({ tracks: json.tracks }))\n                .catch(error => console.log(error))\n            }\n        })\n        .catch(error => console.log(error))\n    }\n\n    render () {\n        return (\n            <div>\n                <h2>Music Master</h2>\n                <input\n                        onChange={this.updateArtistQuery}\n                        onKeyPress={this.handleKeyPress}\n                        placeholder='Search for an Artist'\n                        />\n                <button onClick={this.searchArtist}>Search</button>\n                <Artist artist={this.state.artist} />\n                <Tracks tracks={this.state.tracks} />\n            </div>\n        )\n    }\n}\n\nexport default App\n"},"lineCount":null}},"error":null,"hash":"32faf5d5611ae5f96ed5658a7a342883","cacheData":{"env":{}}}